# StudentService-deployment.yaml
apiVersion: apps/v1
kind: Deployment 
metadata:
  name: school-v1
spec:
  replicas: 1 
  selector:  
    matchLabels:  
      app: schoolapp
      version: "v1"
  template:  
    metadata: 
      labels:
        app: schoolapp
        version: "v1" 
    spec:
      containers: 
      - name: schoolapp 
        image: abucontainerregistry.azurecr.io/studentservice:v1 
        ports:
        - containerPort: 8900
          name: https     
        
---
# StudentService-service.yaml
apiVersion: v1
kind: Service
metadata: 
  name: studentservice
  labels: 
    app: studentapp
spec:
  ports:
  - port: 8900
  # targetPort: 8900
    name: https
  selector: 
    app: studentapp
        